{
  "name": "lect",
  "version": "0.16.12",
  "description": "Maintenance CLI for internal consumption",
  "keywords": [
    "amend",
    "automagically",
    "automatically",
    "change",
    "cli",
    "cli-app",
    "edit",
    "file",
    "files",
    "latest",
    "lect",
    "maintain",
    "maintenance",
    "programatically",
    "readme",
    "refresh",
    "tool",
    "update"
  ],
  "homepage": "https://codsen.com/os/lect/",
  "repository": {
    "type": "git",
    "url": "https://github.com/codsen/codsen.git",
    "directory": "packages/lect"
  },
  "license": "MIT",
  "author": {
    "name": "Roy Revelt",
    "email": "roy@codsen.com",
    "url": "https://codsen.com"
  },
  "bin": {
    "lect": "cli.js"
  },
  "scripts": {
    "ci_test": "npm run format && tap --no-only --reporter=silent --output-file=testStats.md && npm run clean_cov",
    "dev": "echo\"\"",
    "devunittest": "npm run dev && ./node_modules/.bin/tap --only -R 'base' && npm run clean_cov",
    "format": "npm run lect && npm run prettier && npm run lint",
    "lect": "lect",
    "lint": "../../node_modules/eslint/bin/eslint.js . --ext .js --fix --config \"../../.eslintrc.json\" --quiet",
    "prettier": "../../node_modules/prettier/bin-prettier.js '*.{js,css,scss,vue,md,ts}' --write --loglevel silent",
    "republish": "npm publish || :",
    "tap": "tap",
    "test": "npm run lint && npm run unittest && npm run format",
    "unittest": "./node_modules/.bin/tap --no-only --output-file=testStats.md --reporter=terse && npm run clean_cov",
    "clean_cov": "../../scripts/leaveCoverageTotalOnly.js"
  },
  "tap": {
    "coverage-report": [
      "json-summary",
      "text"
    ],
    "nyc-arg": [
      "--exclude=**/*.cjs.js",
      "--exclude=**/*.umd.js"
    ],
    "timeout": 0
  },
  "lect": {
    "licence": {
      "extras": [
        ""
      ]
    },
    "various": {
      "devDependencies": [
        "@types/lodash.camelcase",
        "@types/lodash.clonedeep",
        "@types/lodash.isequal",
        "@types/lodash.isplainobject",
        "@types/lodash.partition",
        "@types/lodash.trim"
      ]
    }
  },
  "dependencies": {
    "array-pull-all-with-glob": "^5.0.12",
    "colorette": "^1.2.2",
    "inquirer": "^8.0.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isplainobject": "^4.0.6",
    "lodash.partition": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "matcher": "^4.0.0",
    "object-merge-advanced": "^12.0.9",
    "object-path": "^0.11.5",
    "pacote": "^11.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.49.0",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.partition": "^4.6.6",
    "@types/lodash.trim": "^4.5.6",
    "eslint": "^7.23.0",
    "lect": "^0.16.12",
    "tap": "^15.0.0",
    "tempy": "^1.0.1",
    "tslib": "^2.1.0"
  }
}
